{
    "m_SGVersion": 1,
    "m_Type": "UnityEditor.ShaderGraph.CustomFunctionNode",
    "m_ObjectId": "731912146a734d73a506d49dbed3008a",
    "m_Group": {
        "m_Id": ""
    },
    "m_Name": "CalculateLightIntensity (Custom Function)",
    "m_DrawState": {
        "m_Expanded": true,
        "m_Position": {
            "serializedVersion": "2",
            "x": -1674.0,
            "y": -27.00000762939453,
            "width": 285.0,
            "height": 278.0
        }
    },
    "m_Slots": [
        {
            "m_Id": "c9cf7e53ebdb4b0bb7d25a8346d443fd"
        },
        {
            "m_Id": "fc66550ed5874521959fdeeb16307282"
        }
    ],
    "synonyms": [
        "code",
        "HLSL"
    ],
    "m_Precision": 0,
    "m_PreviewExpanded": true,
    "m_DismissedVersion": 0,
    "m_PreviewMode": 0,
    "m_CustomColors": {
        "m_SerializableColors": []
    },
    "m_SourceType": 1,
    "m_FunctionName": "CalculateLightIntensity",
    "m_FunctionSource": "",
    "m_FunctionSourceUsePragmas": true,
    "m_FunctionBody": "#ifdef SHADERGRAPH_PREVIEW\r\n    TotalLight = 1; // 프리뷰에서는 그냥 보이게 설정\r\n#else\r\n    float lightIntensity = 0;\r\n\r\n    // 1. 메인 라이트 계산 (선택 사항, 달빛 등)\r\n    Light mainLight = GetMainLight();\r\n    float mainAtten = mainLight.distanceAttenuation * mainLight.shadowAttenuation;\r\n    // mainLight가 비추는 각도 계산 (NdotL)이 필요하면 추가하지만, 루미놀은 빛만 닿으면 반응하므로 감쇠만 봅니다.\r\n    // 여기서는 스포트라이트(추가 라이트)에 집중하기 위해 메인 라이트는 약하게 하거나 뺄 수 있습니다.\r\n\r\n    // 2. 추가 라이트(스포트라이트, 포인트라이트) 반복 계산\r\n    int pixelLightCount = GetAdditionalLightsCount();\r\n    for (int i = 0; i < pixelLightCount; ++i)\r\n    {\r\n        Light light = GetAdditionalLight(i, WorldPos);\r\n        // 빛의 거리 감쇠 및 그림자 감쇠\r\n        float attenuation = light.distanceAttenuation * light.shadowAttenuation;\r\n        // 빛의 색상 밝기(Luminance)와 감쇠를 곱해서 강도에 누적\r\n        lightIntensity += length(light.color) * attenuation;\r\n    }\r\n\r\n    TotalLight = lightIntensity;\r\n#endif"
}

